a = input()
schetchik = 0
save_num_letter = []
num_letter = 0
b = len(a) - 1
while schetchik < len(a):
    for i in a:

        schetchik +=1
        num_letter +=1

        if schetchik == len(a):
            save_num_letter.append(num_letter)
        if i == ' ':
            num_letter -= 1
            save_num_letter.append(num_letter)
            num_letter = 0
